<h1>投稿作成</h1>
<%= form_with model: @post do |form| %>
  <%= render 'shared/error_messages', object: form.object %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :body %>
  <%= form.text_area :body %>

  <%= form.label :images %>
  <%= form.file_field :images, multiple: true %>
  <%= form.hidden_field :images_cache %>

  <%= form.submit t('.push') %>
<% end %>